{
  "builds": [
    {
      "src": "PetMet/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "installCommand": "pip install --upgrade pip && pip install -r requirements.txt"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "PetMet/wsgi.py"
    }
  ]
}
